{
   "permissions": [ "clipboardWrite" ],
   "background":{"page":"maketxt.html"},
   "options_page": "set.html",
   "browser_action":{
      "default_icon":"/img/icon_32.png",
      "default_title":"航海见识墨水"
   },
    "content_scripts": [
	    {
	      "matches": [ "http://*/*", "https://*/*" ],
	      "exclude_globs": ["http://www.flickr.com/*", "https://www.flickr.com/*"],
	      "js": ["/script/get_sel.js"],
	      "run_at": "document_end"
	    },
	    {
	      "matches": [ "http://www.flickr.com/*", "https://www.flickr.com/*"],
	      "js": ["/script/jquery.min.js","/script/get_flickr.js","/script/get_flickr_api.js","/script/webtoolkit.md5.js"],
	      "run_at": "document_end"
	    },
	    {
	      "matches": [ "http://see.sl088.com/w/*"],
	      "js": ["/script/ink_inject.js"],
	      "run_at": "document_end"
	    }
    ],
   "description":"森亮号航海见识墨水！吸取Web和Flickr，一种微妙的墨水！",
   "icons":{
      "128":"/img/icon_128.png",
      "48":"/img/icon_48.png"
   },
   "name":"见识墨水",
   "permissions":[
	"storage",
      "tabs"
   ],
   "homepage_url":"http://see.sl088.com",
   "manifest_version": 2,
   "version":"2.15.15",
   "minimum_chrome_version":"20"

}